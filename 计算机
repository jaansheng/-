#include "stdio.h"
int main()
{
	char ch[] = "3+4";
	int s;
	int a= ch[0] - '0';
	int b = ch[2] - '0';
	s = a + b;
	printf("字符串表达式=%d", s);
	return 0;
}


#include “stdio.h”
#include “string.h”
int main（） {
char ch[] = “1+2+2+1+2+5+4-1-3+4-8”;
int sum = ch[0] - '0';
for （int i = 1; i < strlen（ch）; i++）
	{
if （ch[i] == '+'）
		{
int a = ch[i + 1] - '0';
sum = sum + a;
i++;
		}
else if （ch[i] == '-'）
		{
int b = ch[i + 1] - '0';
sum = sum - b;
i++;
		}
	}
printf（“%d\n”， sum）;
return 0;
}



#include “stdio.h”
#include “string.h”
int main（）
{
char ch[] = “2*2/4*1/1*2*3/2”;
int s = ch[0] - '0';
for （int i = 1; i < strlen（ch）; i++）
	{
if （ch[i] == '*'）
		{
int a = ch[i + 1] - '0';
s = s * a;
i++;
		}
else if （ch[i] == '/'）
		{
int b = ch[i + 1] - '0';
s = s / b;
i++;
		}
	}
printf（“%d\n”， s）;
return 0;
}

#include “stdio.h”
#include “string.h”
int main()
{
	char ch[100]= "2+2*3+2/2+3", ch1[5] = "+-*/";
	int ch2[4] = { 0 };
	int sum = 0, n = 0, m = 0, j;

	while ('\0' != ch[i])
	{
		n = tiqu(ch);

		fuhao(ch[i], ch2);

		m = tiqu(ch);

		for (j = 0; j < 4; j++)
			if (ch2[j] == 1)
			{
				switch (ch1[j])
				{
				case '+': sum += n + m; ch2[j] = 0; break;
				case '-': sum += n - m; ch2[j] = 0; break;
				case '*':
					if (n != 0)
					{
						sum += n * m; ch2[j] = 0; break;
					}
					else
					{
						sum *= m; ch2[j] = 0; break;
					}
				case '/':
					if (n != 0)
					{
						sum += n / m; ch2[j] = 0; break;
					}
					else
					{
						sum /= m; ch2[j] = 0; break;
					}
				}
			}
		n = 0, m = 0;
	}
	printf("   %d\n", sum);

}


#include <stdio.h>
#include <stdlib.h>
main()
{
	char ch[100];
	gets(ch);
	char hh[10];
	char kh[50];
	double num[51];
	double sum=0;
	int i,j=0,m=0,n=0;
	for(i=0;;i++)
	{
		if(ch[i]>='0'&&ch[i]<='9'||ch[i]=='.')
		{
			hh[j++]=ch[i];
		}
		else
		{
			num[n++]=atof(hh);
			kh[m++]=ch[i];
			j=0;
		}
		if(ch[i]=='\0')
            break;
	}
	puts(kh);
	for(i=0;i<n;i++)
	    printf("%lf ",num[i]); 
    //fenli
    for(i=0;kh[i];i++)
    {
    	if(kh[i]=='*')
    	{
	    	num[i]=num[i]*num[i+1];
	    	for(n=i+1;n<50;n++)
	    	    num[n]=num[n+1];
            for(n=i;kh[n];n++)
                kh[n]=kh[n+1];
	    }
	    if(kh[i]=='/')
    	{
	    	num[i]=num[i]/num[i+1];
	    	for(n=i+1;n<50;n++)
	    	    num[n]=num[n+1];
            for(n=i;kh[n];n++)
                kh[n]=kh[n+1];
	    }
    }
    sum=num[0];
    for(i=0;kh[i];i++)
    {
    	if(kh[i]=='+')
    	sum=sum+num[i+1];
		if(kh[i]=='-')
    	sum=sum-num[i+1];
    }
    printf("\n%lf ",sum);
}
